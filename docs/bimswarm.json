        "/proxy/api/v0/toolchainservice/toolchainitem/{itemId}/status": {
            "put": {
                "summary": "[2] [6] UpdateToolchainItemStatus",
                "description": "App A posts status \"WORKING\" to the BIMSWARM tool\nchain API\n* The itemId must be specified here for the product from one of the toolchain instances\nselected in request 1.\n* The status must be a valid status from the item status enumeration. At this point\nmust be sent according to status transitions \"WORKING\".",
                "operationId": "[2][6]UpdateToolchainItemStatus",
                "parameters": [{
                    "$ref": "#/components/parameters/itemId"
                }],
                "responses": {
                    "200": {
                        "description": "[2] [6] UpdateToolchainItemStatus",
                        "content": {
                            "text/plain;charset=UTF-8": {
                                "schema": {}
                            }
                        }
                    }
                },
                "requestBody": {
                    "description": "App A posts status \"WORKING\" to the BIMSWARM tool\nchain API\n* The itemId must be specified here for the product from one of the toolchain instances\nselected in request 1.\n* The status must be a valid status from the item status enumeration. At this point\nmust be sent according to status transitions \"WORKING\".",
                    "required": true,
                    "content": {
                        "text/plain": {
                            "schema": {
                                "type": "string",
                                "example": "\"PENDING\" / \"SUCCES\" / \"WORKING\" / \"FAILED\""
                            },
                            "example": "\"PENDING\" / \"SUCCES\" / \"WORKING\" / \"FAILED\""
                        }
                    }
                }
            }
        },
        "/proxy/api/v0/toolchainservice/ocdecontent/{ocdeContentId}": {
            "get": {
                "summary": "[3] GetOcdeContentDetails",
                "description": "App A queries all ContentIDs from the BIMSWARM\nOpenCDE\n* the ocdeContentIds are stored in the ActivityItem for every incoming and outgoing flow\n. The OcdeContents request returns the\n external ID\nrelevant to the file transfer. This is needed for requests 4 and 5.",
                "operationId": "[3]getOcdeContentDetails",
                "parameters": [{
                    "$ref": "#/components/parameters/ocdeContentId"
                }],
                "responses": {
                    "200": {
